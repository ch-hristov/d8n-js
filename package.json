  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ch-hristov/d8n-js/issues"
  },
  "homepage": "https://github.com/ch-hristov/d8n-js#readme",
  "devDependencies": {
    "prettier": "^2.8.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "files": ["lib/**/*"],
  "keywords": ["Engineering drawings", "P&ID", "Document intelligence", "Object detection"],
  "author": "d8n"
}
